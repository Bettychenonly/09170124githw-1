    "                Mid_Course_y.append(item2[\"numSoldTickets\"])\n",
    "            else:\n",
    "                Long_Course_x.append(item2[\"totalVideoLengthInSeconds\"])\n",
    "                Long_Course_y.append(item2[\"numSoldTickets\"])\n",
    "print(\"『共有\",len(All_xx),\"種課程』\") \n",
    "print(\"《加分題》\")\n",
    "print(\"Q_Bonus: '課程時長' 會影響 '報名人數' 的多寡嗎？ \")\n",
    "plt.figure(figsize=(20,10))   # 顯示圖框架大小\n",
    "plt.style.use(\"ggplot\")                      \n",
    "plt.xlabel(\"CourseVideoLength\", fontweight = \"bold\")                  #設定x座標標題及粗體\n",
    "plt.ylabel(\"Number Of Registered\", fontweight = \"bold\")   #設定y座標標題及粗體\n",
    "plt.title(\"Scatter of CourseVideoLength and Number Of Registered\",\n",
    "          fontsize = 15, fontweight = \"bold\")        #設定標題、字大小及粗\n",
    "plt.scatter(Short_Course_x,                              # x軸資料\n",
    "            Short_Course_y,                              # y軸資料\n",
    "            c = \"green\",                                  # 點顏色\n",
    "            s = 50,                                   # 點大小\n",
    "            alpha = 1,\n",
    "            marker=\"o\",\n",
    "            label = \"Less than 90 min\")                               # 透明度    \n",
    "plt.scatter(Mid_Course_x,                              # x軸資料\n",
    "            Mid_Course_y,                              # y軸資料\n",
    "            c = \"brown\",                                  # 點顏色\n",
    "            s = 60,                                  # 點大小\n",
    "            alpha = .5,                               # 透明度\n",
    "            marker = \"p\",\n",
    "            label = \"91~180 min \")\n",
    "plt.scatter(Long_Course_x,                              # x軸資料\n",
    "            Long_Course_y,                              # y軸資料\n",
    "            c = \"orange\",                                  # 點顏色\n",
    "            s = 50,                                   # 點大小\n",
    "            alpha = 1,                               # 透明度\n",
    "            marker = \"s\",\n",
    "            label = \"Over 180 min\") \n",
    "plt.legend()\n",
    "plt.show()\n",
    "Q11=np.corrcoef(All_xx,All_yy)\n",
    "print(Q11)\n",
    "print(\"Ans_Bonus:在所有課程中，'報名人數' 與 '課程價格' 兩者間的相關係數約為-0.129，呈現低度負相關，不正相關也不太負相關，因此我認為課程時長不會影響報名人數的多寡。\")"
